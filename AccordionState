//accordion state
ele.outerFind('#fsEl_138704 .schools-toggle').each(function () {

  // Set initial aria-expanded state
  $ae(this).attr('aria-expanded', 'false');

  $ae(this).on('click', function () {
    let isExpanded = $ae(this).attr('aria-expanded') === 'true';

    // Toggle the aria-expanded attribute
    $ae(this).attr('aria-expanded', !isExpanded);
  });
});
